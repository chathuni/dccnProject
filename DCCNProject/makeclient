client.out: error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o tcpcli.o readline.o writen.o strcliselect.o
	cc -o client.out strcliselect.o error.o wrapunix.o wrapsock.o wraplib.o wrapstdio.o tcpcli.o readline.o writen.o

error.o: error.c utils.h
	cc -c error.c

wrapunix.o: wrapunix.c utils.h
	cc -c wrapunix.c

wrapsock.o: wrapsock.c utils.h
	cc -c wrapsock.c

wraplib.o: wraplib.c utils.h
	cc -c wraplib.c

wrapstdio.o: wrapstdio.c utils.h
	cc -c wrapstdio.c

tcpcli.o: tcpcli.c utils.h
	cc -c tcpcli.c

strcliselect.o: strcliselect.c utils.h
	cc -c strcliselect.c

readline.o: readline.c utils.h
	cc -c readline.c

writen.o: writen.c utils.h
	cc -c writen.c
